{
  "name": "typescriptase",
  "version": "0.0.5",
  "description": "TypeScript Codegen",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "repository": "https://github.com/arichiv/typescriptase.git",
  "author": "Ari Chivukula <ari@chivuku.la>",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.46",
    "glob": "^7.1.2",
    "npm": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@types/glob": "^5.0.35",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.6",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "bin": {
    "typescriptase": "bin/cli.js"
  },
  "scripts": {
    "build": "tsc && babel --minified bin/**/*.js -d bin/ && chmod +x bin/cli.js",
    "lint": "tslint -p .",
    "snap": "jest -u",
    "test": "jest && ts-node src/cli.ts **/setup.ts"
  }
}
