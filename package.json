{
  "name": "typescriptase",
  "version": "0.6.0",
  "description": "TypeScript Codegen",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "repository": "https://github.com/AriChivukula/typescriptase.git",
  "author": "Ari Chivukula <ari@chivuku.la>",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.49",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.4",
    "@types/yargs": "^11.0.0",
    "jest": "^23.1.0 ",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "bin": {
    "typescriptase": "bin/cli.js"
  },
  "scripts": {
    "prep": "rm package-lock.json && rm -rf node_modules && npm install",
    "codegen": "ts-node src/cli.ts --files gen/*.ts",
    "build": "tsc && babel --minified bin/*.js -d bin/",
    "lint": "tslint -p .",
    "snap": "jest -u",
    "test": "jest && ts-node src/cli.ts --expectNoChanges --files gen/*.ts"
  }
}
